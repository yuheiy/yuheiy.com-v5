---
permalink: "{{ page.filePathStem }}.html"
---
<!DOCTYPE html>
<html lang="{{ metadata.lang }}">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width">
		<title>{{ title }} - {{ metadata.title }}</title>
		<meta name="description" content="{{ description | default: metadata.description }}">
		<meta name="twitter:card" content="summary">
		<meta property="og:title" content="{{ title }}">
		<meta property="og:type" content="{{ type | default: "website" }}">
		<meta property="og:image" content="{{ metadata.origin }}/yuheiy.png">
		<meta property="og:url" content="{{ metadata.origin }}{{ page.url | prettyUrl }}">
		<meta property="og:description" content="{{ description | default: metadata.description }}">
		<meta property="og:site_name" content="{{ metadata.title }}">
		<meta property="og:locale" content="{{ metadata.lang }}_{{ metadata.region }}">
		<meta name="generator" content="{{ metadata.generator }}">
		<link rel="canonical" href="{{ metadata.origin }}{{ page.url | prettyUrl }}">
		<link rel="apple-touch-icon" href="/yuheiy.png">
		<link rel="alternate" href="{{ metadata.feed.path }}" type="application/atom+xml" title="{{ metadata.feed.title }}">
		<link rel="alternate" href="{{ metadata.jsonfeed.path }}" type="application/json" title="{{ metadata.jsonfeed.title }}">
		<style>
			{% capture styles %}{% include main.css %}{% endcapture %}
			{{ styles }}
		</style>
	</head>

	<body>
		<svg display="none">
			<symbol id="icon-mail" viewBox="0 0 24 24">
				<path d="M0 0h24v24H0z" fill="none"/>
				<path d="M20 4H4c-1.1 0-1.99.9-1.99 2L2 18c0 1.1.9 2 2 2h16c1.1 0 2-.9 2-2V6c0-1.1-.9-2-2-2zm0 14H4V8l8 5 8-5v10zm-8-7L4 6h16l-8 5z"/>
			</symbol>
			<symbol id="icon-history" viewBox="0 0 24 24">
				<path d="M0 0h24v24H0z" fill="none"/>
				<path d="M13 3c-4.97 0-9 4.03-9 9H1l3.89 3.89.07.14L9 12H6c0-3.87 3.13-7 7-7s7 3.13 7 7-3.13 7-7 7c-1.93 0-3.68-.79-4.94-2.06l-1.42 1.42C8.27 19.99 10.51 21 13 21c4.97 0 9-4.03 9-9s-4.03-9-9-9zm-1 5v5l4.28 2.54.72-1.21-3.5-2.08V8H12z"/>
			</symbol>
			<symbol id="icon-rss" viewBox="0 0 24 24">
				<path d="M0 0h24v24H0z" fill="none"/>
				<circle cx="6.18" cy="17.82" r="2.18"/>
				<path d="M4 4.44v2.83c7.03 0 12.73 5.7 12.73 12.73h2.83c0-8.59-6.97-15.56-15.56-15.56zm0 5.66v2.83c3.9 0 7.07 3.17 7.07 7.07h2.83c0-5.47-4.43-9.9-9.9-9.9z"/>
			</symbol>
			<symbol id="icon-arrow-back" viewBox="0 0 24 24">
				<path d="M0 0h24v24H0z" fill="none"/>
				<path d="M11.67 3.87L9.9 2.1 0 12l9.9 9.9 1.77-1.77L3.54 12z"/>
			</symbol>
			<symbol id="icon-chevron-right" viewBox="0 0 24 24">
				<path d="M0 0h24v24H0z" fill="none"/>
				<path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z"/>
			</symbol>
			<symbol id="icon-twitter" viewBox="0 0 400 400">
				<circle cx="200" cy="200" r="200" fill="#1da1f2"/>
				<path d="M163.4 305.5c88.7 0 137.2-73.5 137.2-137.2 0-2.1 0-4.2-.1-6.2 9.4-6.8 17.6-15.3 24.1-25-8.6 3.8-17.9 6.4-27.7 7.6 10-6 17.6-15.4 21.2-26.7-9.3 5.5-19.6 9.5-30.6 11.7-8.8-9.4-21.3-15.2-35.2-15.2-26.6 0-48.2 21.6-48.2 48.2 0 3.8.4 7.5 1.3 11-40.1-2-75.6-21.2-99.4-50.4-4.1 7.1-6.5 15.4-6.5 24.2 0 16.7 8.5 31.5 21.5 40.1-7.9-.2-15.3-2.4-21.8-6v.6c0 23.4 16.6 42.8 38.7 47.3-4 1.1-8.3 1.7-12.7 1.7-3.1 0-6.1-.3-9.1-.9 6.1 19.2 23.9 33.1 45 33.5-16.5 12.9-37.3 20.6-59.9 20.6-3.9 0-7.7-.2-11.5-.7 21.1 13.8 46.5 21.8 73.7 21.8" fill="#fff"/>
			</symbol>
			<symbol id="icon-github" viewBox="0 0 16 16">
				<path fill-rule="evenodd" d="M8 0C3.58 0 0 3.58 0 8c0 3.54 2.29 6.53 5.47 7.59.4.07.55-.17.55-.38 0-.19-.01-.82-.01-1.49-2.01.37-2.53-.49-2.69-.94-.09-.23-.48-.94-.82-1.13-.28-.15-.68-.52-.01-.53.63-.01 1.08.58 1.23.82.72 1.21 1.87.87 2.33.66.07-.52.28-.87.51-1.07-1.78-.2-3.64-.89-3.64-3.95 0-.87.31-1.59.82-2.15-.08-.2-.36-1.02.08-2.12 0 0 .67-.21 2.2.82.64-.18 1.32-.27 2-.27.68 0 1.36.09 2 .27 1.53-1.04 2.2-.82 2.2-.82.44 1.1.16 1.92.08 2.12.51.56.82 1.27.82 2.15 0 3.07-1.87 3.75-3.65 3.95.29.25.54.73.54 1.48 0 1.07-.01 1.93-.01 2.2 0 .21.15.46.55.38A8.013 8.013 0 0016 8c0-4.42-3.58-8-8-8z" fill="#24292e"></path>
			</symbol>
		</svg>

		<div class="page">
			<header class="header center pt-1 pb-1 text-sm color-text-muted weight-bold">
				<div class="cluster cluster-justify-between cluster-space-x-5 cluster-space-y-1">
					<p>
						<a class="header__logo button" {%if page.url != "/"%}href="/"{%endif%}>
							<span class="with-icon with-icon-space">
								<img class="icon" src="/yuhei.png" width="256" height="256" alt="" decoding="async">
								<span>{{ metadata.title }}</span>
							</span>
						</a>
					</p>
					<address>
						{% include social-list %}
					</address>
				</div>
			</header>

			<div class="divider"></div>

			<div class="center">
				{{ content }}
			</div>

			<footer class="center mt-5 text-sm color-text-muted weight-bold">
				<p class="cluster cluster-justify-end">
					<a class="button" href="{{ metadata.repositoryUrl }}/commits/master{{ page.inputPath | remove_first: "." }}">
						<span class="with-icon with-icon-space">
							<svg class="icon color-primary" viewBox="0 0 1 1"><use href="#icon-history"></svg>
							<span>編集履歴</span>
						</span>
					</a>
				</p>
			</footer>

			<footer class="footer center mt-3 pt-3 pb-3 text-sm color-text-muted weight-bold">
				<div class="cluster cluster-justify-between cluster-space-x-5 cluster-space-y-1">
					<div>
						<ul class="cluster cluster-space-y-0">
							<li><a class="button" href="/">ホーム</a></li>
							<li><a class="button" href="{{ "/posts.html" | prettyUrl }}">投稿</a></li>
							<li><a class="button" href="{{ metadata.repositoryUrl }}">ソースコード</a></li>
						</ul>
					</div>
					<address>
						{% include social-list %}
					</address>
				</div>
			</footer>
		</div>
	</body>
</html>
